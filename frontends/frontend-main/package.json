{
  "name": "ante-frontend",
  "version": "0.0.1",
  "description": "Integrated ERP system that streamlines accounting, inventory, HR, and moreâ€”boosting efficiency, accuracy, and business growth.",
  "productName": "Ante",
  "author": "Guillermo Tabligan <ceo@geer.solutions>",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "rm -rf screenshots && playwright test",
    "test:clean": "rm -rf screenshots",
    "test:supplier": "playwright test supplier-table.spec.ts",
    "test:quick": "playwright test quick-supplier-check.spec.ts",
    "test:install": "playwright install",
    "test:report": "playwright show-report",
    "test:login-task": "playwright test login-to-task.spec.ts",
    "test:login-task:headed": "playwright test login-to-task.spec.ts --headed",
    "test:login-task:debug": "playwright test login-to-task.spec.ts --debug",
    "test:login-task:slow": "playwright test login-to-task.spec.ts --headed --timeout=120000",
    "test:workflow": "playwright test continuous-workflow.spec.ts",
    "test:workflow:headed": "playwright test continuous-workflow.spec.ts --headed",
    "test:workflow:debug": "playwright test continuous-workflow.spec.ts --debug",
    "test:task": "playwright test task-creation.spec.ts",
    "test:task:headed": "playwright test task-creation.spec.ts --headed",
    "test:assignment": "playwright test task-assignment-modes.spec.ts",
    "test:assignment:headed": "playwright test task-assignment-modes.spec.ts --headed",
    "dev": "NODE_OPTIONS='--max-old-space-size=4096' quasar dev",
    "dev:low": "MEMORY_MODE=low NODE_OPTIONS='--max-old-space-size=2048' quasar dev --no-lint",
    "dev:minimal": "MEMORY_MODE=minimal NODE_OPTIONS='--max-old-space-size=1024' quasar dev --no-lint",
    "dev:https": "HTTPS=true quasar dev",
    "start:dev": "quasar dev",
    "build": "NODE_OPTIONS='--max-old-space-size=6144' quasar build",
    "prepare": "husky"
  },
