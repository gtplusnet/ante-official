# API Configuration (Removed - now using Supabase directly)
# NEXT_PUBLIC_API_URL - No longer needed, using Supabase
NEXT_PUBLIC_API_VERSION=v1

# Company Configuration (Required for multi-tenant setup)
# This ID identifies your company in the backend database
NEXT_PUBLIC_COMPANY_ID=1

# Application Configuration
NEXT_PUBLIC_APP_NAME="Geer Guardian"
NEXT_PUBLIC_APP_VERSION="1.0.0"

# Feature Flags
NEXT_PUBLIC_ENABLE_BIOMETRIC_AUTH=false
NEXT_PUBLIC_ENABLE_PUSH_NOTIFICATIONS=false

# Supabase Configuration (Required for database and realtime)
# NEXT_PUBLIC_SUPABASE_URL=your-supabase-project-url
# NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key

# Future Integration (Not yet implemented)
# NEXT_PUBLIC_FIREBASE_API_KEY=your-firebase-api-key
# NEXT_PUBLIC_FIREBASE_PROJECT_ID=your-firebase-project-id
# NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=your-firebase-messaging-sender-id
# NEXT_PUBLIC_FIREBASE_APP_ID=your-firebase-app-id
# NEXT_PUBLIC_WS_URL - Removed, now using Supabase realtime
# NEXT_PUBLIC_JWT_SECRET=your-jwt-secret